<div class="container">
  <div class="row">
    <div class="col-md-6 col-md-offset-3">
      <% if not @filled_form %>
        <div class="panel panel-group">
          <%= render 'form' %>
        </div>
      <% else %>
        <div class="panel panel-primary">
          <div class="panel-heading">Successfully generated label!</div>
          <div class="panel-body">
            Link to print label: <a href=<%= @shipment_url %>>Label</a>
          </div>
        </div>
      <% end %>
      </div>
    </div>
  </div>
</div>
